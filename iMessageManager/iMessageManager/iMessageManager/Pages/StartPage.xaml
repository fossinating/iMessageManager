<UserControl x:Class="iMessageManager.StartPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:iMessageManager"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition></ColumnDefinition>
            <ColumnDefinition></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition></RowDefinition>
            <RowDefinition></RowDefinition>
        </Grid.RowDefinitions>
        <Button x:Name="searchButton" Content="Search" Grid.Column="0" Grid.Row="0" Click="searchButton_Click"/>
        <Button x:Name="statsButton" Content="Stats" Grid.Column="1" Grid.Row="0"/>
        <Button x:Name="graphsButton" Content="Graphs" Grid.Column="0" Grid.Row="1"/>
        <Button x:Name="settingsButton" Content="Settings" Grid.Column="1" Grid.Row="1" Click="settingsButton_Click"/>
    </Grid>
</UserControl>
